{
  "name": "seconder.ai",
  "version": "11.0.0",
  "description": "",
  "engines": {
    "node": "23.11.0"
  },
  "scripts": {
    "prepare": "husky",
    "prestart:default": "$NVM_DIR/versions/node/v$npm_package_engines_node/bin/pnpm up",
    "prestart:windows": "%NVM_HOME%/v%npm_package_engines_node%/pnpm up",
    "prestart": "node run-script-os.cjs",
    "start": "pnpm run prestart && turbo run start --concurrency=50",
    "preturbo": "rm -rf ./out",
    "turbo:prune": "pnpm run preturbo && turbo prune --docker",
    "lint:js": "turbo run lint:js --",
    "lint:css": "turbo run lint:css --",
    "lint": "eslint --fix",
    "release": "semantic-release --ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "semantic-release": "^24.2.3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@sec/eslint-config": "workspace:*",
    "@sec/tsconfig": "workspace:*",
    "@turbo/gen": "^2.5.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "stylelint": "16.17.0",
    "turbo": "^2.5.0"
  },
  "dependencies": {
    "express": "^5.1.0",
    "turbo": "2.5.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix"
    ]
  },
  "type": "module"
}
