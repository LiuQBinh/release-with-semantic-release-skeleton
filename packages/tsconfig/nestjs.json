{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "common nestjs tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2020",
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": false
  }
}
