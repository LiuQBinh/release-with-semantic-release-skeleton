{
  "name": "gui.seconder-webapp-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prestart": "rimraf dist",
    "start:extension": "concurrently \"vite build --watch\" \"vite build --watch --config src/extension/modules/content/vite.config.ts\"",
    "start:webapp": "vite",
    "start": "pnpm run start:extension & pnpm run start:webapp",
    "preaws:build": "rimraf dist",
    "aws:build:extension": "tsc -b && vite build && vite build --config src/extension/modules/content/vite.config.ts",
    "aws:build:webapp": "tsc -b && vite build",
    "aws:build": "pnpm run aws:build:extension && pnpm run aws:build:webapp",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@sec/gui-i18n-localization": "workspace:*",
    "@sec/gui-seconder": "workspace:*",
    "@tailwindcss/vite": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "7.5.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4",
    "use-intl": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@sec/eslint-config": "workspace:*",
    "@sec/stylelint-config": "workspace:*",
    "@sec/tsconfig": "workspace:*",
    "@types/chrome": "0.0.304",
    "@types/node": "^22.15.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tw-animate-css": "^1.2.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "concurrently": "^9.1.0",
    "rimraf": "^6.0.1"
  }
}
